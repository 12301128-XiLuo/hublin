{
  "app": {
    "name": "Linagora Meetings"
  },
  "webserver": {
    "enabled": true,
    "debugPort": 8081,

    "ip": "0.0.0.0",
    "ipv6": "::",
    "port": 8080,

    "ssl_ip": "0.0.0.0",
    "ssl_ipv6": "::",
    "ssl_port": 8443,
    "ssl_key": "config/ssl.key",
    "ssl_cert": "config/ssl.crt",
    "ssl_ca": [],

    "virtualhosts": [],
    "startupBufferTimeout": 5000
  },
  "wsserver": {
    "enabled": true,
    "port": 8080
  },
  "webrtc": {
    "enabled": true,
    "level": "debug",
    "roomNameRegExp": "^[a-z0-9_.-]{1,64}$",
    "usernameRegExp": "^(.){1,200}$"
  },
  "loggers" : [
    {
      "name": "File",
      "enabled": true,
      "options": {
        "filename": "./log/application.log",
        "level": "info",
        "handleExceptions": true,
        "json": false,
        "prettyPrint": true,
        "colorize": false
      }
    },
    {
      "name": "Console",
      "enabled": true,
      "options": {
        "enabled": true,
        "level": "info",
        "handleExceptions": true,
        "json": false,
        "prettyPrint": true,
        "colorize": true
      }
    }
  ],
  "emailInvitation": {
    "templatesDir": "./templates/email"
  },
  "modules": [
    "linagora.io.meetings"
  ]
}
